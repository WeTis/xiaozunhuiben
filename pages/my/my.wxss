/* pages/my/my.wxss */.page {
  background-color: #fff;
  overflow: auto;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  overflow-x: hidden;
}

.profile-header {
  width: 100%;
  height: 315rpx;
  background-color: #fff;
  overflow: hidden;
  position: relative;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.topNavBar {
  background-color:rgba(0, 0, 0, 0) !important;
}
.avatar-url {
  width: 100rpx;
  height: 100rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
  position: absolute;
  bottom: 58rpx;
  left: 30rpx;
}

.nickname {
  position: absolute;
  left: 150rpx;
  bottom: 100rpx;
  height: 42rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #fff;
  font-weight: 400;
}
.leve{
  position: absolute;
  left: 150rpx;
  bottom: 58rpx;
  height: 42rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #fff;
  font-weight: 400;
}
.member{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 80rpx;
  color: #fff;
  background-color: greenyellow;
  height: 200rpx;
  border-radius: 20rpx;
  margin: 0 30rpx;
  margin-top: 30rpx;
}

.info{
  position: absolute;
  right: -10rpx;
  bottom: 100rpx;
  width: 160rpx;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  background: linear-gradient(-45deg, #4C4844, #7C7470);
  border: 1px solid #FFFFFF;
  border-radius: 10rpx;
}
.info text{  
  font-size: 24rpx;
  padding-left: 10rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #FFFFFF;
}
.info i{
  font-size: 20rpx;
  color: #fff;
  padding-left: 10rpx;
}


.icons{
  width: 100%;
  padding: 30rpx;
  padding-bottom: 15rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 0;
  background-color: #fff;
}
.icons .icons-item{
  width: 25%;
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 10rpx 0;
  margin-bottom: 40rpx;
  color: #232629;
}
.icons .icons-item i {
  font-size: 45rpx;
  margin-bottom: 23rpx;
  color: #2AB1E7;
}


.toolBox{
  width: 100%;
  background-color: #f1f1f1;
  padding-top: 20rpx;
}
.toolBox .title{
  background-color: #fff;
  padding-left: 31rpx;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #666666;
  /* border-bottom: 2rpx solid #E5E5E5; */
}
.ls{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: #fff;
}
.ls .ls-item{
  width: 187rpx;
  height: 177rpx;
  display: flex;
  box-sizing: border-box;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  /* border-bottom: 2rpx solid #E5E5E5;
  border-right: 2rpx solid #E5E5E5; */
}
.ls .ls-item:nth-child(4n){
  border-right:none;
}
.ls .ls-item .desc{
  margin-top: 17rpx;
  font-size: 24rpx;
  font-family: PingFang;
  font-weight: 500;
  color: #333333;
}
/* 导航 */

.dialog{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.7);
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: center;
    justify-content: center;
}
.dialog .content{
  width: 640rpx;
  border-radius: 18rpx;
  background-color: #fff;
  padding: 40rpx;
  padding-bottom: 0;
  box-sizing: border-box;
}
.dialog .title{
  font-weight: 500;
  line-height: 48rpx;
  text-align: center;
}
.dialog .text{
  padding: 20rpx 0;
}
.diaLogBtn{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.diaLogBtn .btn{
  flex: 1;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  color: #323233;
  font-size: 32rpx;
  position: relative;
}
 .diaLogBtn .btn button{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  opacity: 0;
  z-index: 1;
  width: 100%;
}
 .card{
   width: 100%;
  /* height: 260rpx; */
  padding: 30rpx 0;
  background-color: #fff;
  box-sizing: border-box;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  position: relative;
 }
 .card::after{
   content: " ";
   display: block;
   width: 100%;
   height: 20rpx;
   position: absolute;
   top: -20rpx;
   left: 0;
   background-color: #f1f1f1;
 }
 .card image{
  width: 690rpx;
}
