#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_mini-program-iconfont-cli@0.6.0@mini-program-iconfont-cli/commands/help.js" "$@"
  ret=$?
else 
  node  "$basedir/../_mini-program-iconfont-cli@0.6.0@mini-program-iconfont-cli/commands/help.js" "$@"
  ret=$?
fi
exit $ret
